{
  "name": "<%= elementName %>",
  "version": "0.0.0",
  "description": "<%= elementDescription %>",
  "homepage": "https://github.com/<%= elementOrganizationName %>/<%= elementName %>",
  "bugs": "https://github.com/<%= elementOrganizationName %>/<%= elementName %>/issues",
  "license": "MIT",
  "author": "<%- elementAuthor %>",
  "repository": "https://github.com/<%= elementOrganizationName %>/<%= elementName %>.git",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "dev": "rollup -cw",
    "prebuild": "yarn clean",
    "build": "rollup -c",
    "postbuild": "copyfiles CHANGELOG.md LICENSE package.json README.md dist",
    "purge": "rm -rf node_modules",
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.*(json|js|ts|yml|md)\"",
    "format:check": "prettier --check \"**/*.*(json|js|ts|yml|md)\"",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "release": "standard-version --no-verify",
    "release:major": "yarn release --release-as major",
    "release:minor": "yarn release --release-as minor",
    "release:patch": "yarn release --release-as patch"
  },
  "devDependencies": {
    "@boringcodes/prettier-config": "*",
    "copyfiles": "^2.2.0",
    "husky": "^2.3.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup": "^1.12.3",
    "standard-version": "^6.0.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-immutable": "^5.5.2",
    "tslint-sonarts": "^1.9.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
